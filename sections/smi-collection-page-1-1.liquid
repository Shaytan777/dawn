                    if forloop.index0 < 2 and section.index < 3
                        assign imageLoading = 'eager'
                    endif
                %}
                {% if preload %}
                    {{ product.featured_media | image_url: width: productImageWidth | image_tag: class: classes, alt: product.title, preload: true , widths: productImageWidths, sizes: productImageSizes, style: productStyle }}
                {% else %}                    
                    {{ product.featured_media | image_url: width: productImageWidth | image_tag: class: classes, alt: product.title, widths: productImageWidths, loading: imageLoading, sizes: productImageSizes, style: productStyle }}
                {% endif %}
                {%- if has_second_image -%}
                    {% assign classes = ' smi-wh-100 smi-p-absolute smi-object-fit-cover smi-top-left-0 smi-opacity-0 smi-d-lazy smi-d-none smi-d-md-block ' | append: media_classes %}
