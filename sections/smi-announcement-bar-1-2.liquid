{% comment %}#component#["settings/slides","settings/color-scheme","settings/section-padding","blocks/announcement","blocks/color-announcement","blocks/background","settings/icon-setting","settings/custom-icons","settings/design-mode","settings/slider-arrows"]#endcomponent#{% endcomment %}
{{ 'smi-swiper-bundle.min.css' | asset_url | stylesheet_tag }}
{{ 'smi-swiper-bundle.min.css' | asset_url | stylesheet_tag }}
<script src="{{ 'smi-swiper-bundle.min.js' | asset_url }}" defer data-id="swiper"></script>
<style>
    {% assign color_announcement_block = section.blocks | where: 'type', 'color' | first %}
{% if color_announcement_block %}
      #shopify-section-{{ section.id }},
      #shopify-section-{{ section.id }} .smi-color-{{ section.settings.color_scheme }},
      #shopify-section-{{ section.id }} .smi-block-custom-color , #shopify-section-{{ section.id }} .color-announcement {
        --color-background: {{ color_announcement_block.settings.background_color.red }}, {{ color_announcement_block.settings.background_color.green }},{{ color_announcement_block.settings.background_color.blue }};        
        {% if color_announcement_block.settings.background_gradient_color == blank %}
            --gradient-background: {{ color_announcement_block.settings.background_color }};
        {% else %}
            --gradient-background: {{ color_announcement_block.settings.background_gradient_color }};
        {% endif %}
        --color-foreground: {{ color_announcement_block.settings.text_color.red }}, {{ color_announcement_block.settings.text_color.green }},{{ color_announcement_block.settings.text_color.blue }};
    }
{% endif %}
    #shopify-section-{{ section.id }} {
        --section-padding-top: {{ section.settings.padding_top }}px;
--section-padding-bottom: {{ section.settings.padding_bottom }}px;
{% if section.settings.padding_top_section != blank or section.settings.padding_bottom_section != blank %}
    --section-margin-top: {{ section.settings.padding_top_section }}px;
    --section-margin-bottom: {{ section.settings.padding_bottom_section }}px;
{% endif %}
    }
    #smi-{{ section.id }} {
    {%- for block in section.blocks -%}{% if block.type == 'background' %} --color-background: {{ block.settings.background_simple }};
        --gradient-background: {{ block.settings.background_simple }};
    {% endif %}{% endfor %}
    }
</style>
{% liquid
    if section.settings.auto_rotate_slides == true
        assign delay = section.settings.change_slides_every | times: 1000
        assign autoplay = '{ "delay": ' | append: delay | append: '}'
    else
        assign autoplay = false
    endif
%}
<div id="smi-{{ section.id }}" class="smi-{{ section.id }} smi-section__inner smi-gradient smi-section-top-bar-slider smi-section-annoucement-bar-1
{% if section.settings.enable_sticky_bar %} sticky_announcement-bar{% endif %}
 smi-color-{{ section.settings.color_scheme }}" data-section="announcement-bar-1" data-name="✧Announcement bar 1">
     
    {% render 'smi-background' %}
    <div class="{% if section.settings.container_full_width %}smi-container-fluid{% else %}smi-container{% endif -%}">
        {% assign announcement_blocks = section.blocks | where: 'type', 'announcement'
        %}
        {% liquid
            if announcement_blocks.size > 1
                assign loop = true
            else
                assign loop = false
            endif
        %}
        <div class="smi-content-announcement-wrapper">
            <div class="swiper smi-swiper smi-announcement-wrapper"
                 data-swiper-options='{"autoplay":{{ autoplay }}, "loop" : {{ loop }}}'>
                <div class="swiper-wrapper">
                    {% for block in section.blocks %}
                        {% if block.type == 'announcement' %}
                            <div class="swiper-slide" {{ block.shopify_attributes }}>
<div class="smi-component-blocks-announcement" {{- block.shopify_attributes -}}>
    <div class="smi-component-blocks-announcement__content smi-link-wrapper smi-d-flex smi-justify-content-center smi-align-items-center smi-link-wrapper">
        {% if block.settings.icon != 'none' %}
            <div class="smi-component-settings-icon-setting- smi-component-settings-icon-setting-{{ block.id }} ">
<style>
    .smi-component-settings-icon-setting-{{ block.id }} span,
    .smi-component-settings-icon-setting-{{ block.id }} img {
        width: {{ block.settings.icon_size }}px;
        height: {{ block.settings.icon_size }}px;
        object-fit: cover;
    }
    .smi-component-settings-icon-setting-{{ block.id }} svg {
        width: {{ block.settings.icon_size }}px;
        height: {{ block.settings.icon_size }}px;
        object-fit: cover;
    }
</style>
<span class="smi-d-block">
    {%- if block.settings.upload_icon != blank -%}
        {{ block.settings.upload_icon | image_url: width: 1000, height: 1000 | image_tag: loading: 'lazy' }}
    {%- else -%}
        {% if block.settings.icon != 'none' and block.settings.icon != "" %}
            {% comment %} <span class="icon-sample smi-collapse-icon-sample smi-mr-xs">{%- render 'smi-custom-icons', icon: block.settings.icon -%}</span> {% endcomment %}
            {%- render 'smi-custom-icons', icon: block.settings.icon -%}
        {% endif %}
    {%- endif -%}
</span>
</div>
        {% endif %}
        {{block.settings.content}}
    </div>
</div>
                            </div>
                        {% endif %}
                    {% endfor %}
                </div>
            </div>
            {% if announcement_blocks.size > 1 %}
                <div class="smi-swiper-controls">
                    <div class="smi-swiper-navigation">
                        {% if section.settings.arrow_style != 'none' %}
  <div class="smi-component-arrow-slider">
    <div class="arrow-slider-content">
      <div class="{{ section.settings.arrow_style }} arrow-icon arrow-slider-left smi-cursor-pointer">
        {% if section.settings.arrow_style == 'arr-style-2' or 
          section.settings.arrow_style == 'arr-square-style-4' or
          section.settings.arrow_style == 'arr-square-style-5' or
          section.settings.arrow_style == 'arr-square-style-6' or
          section.settings.arrow_style == 'arr-circle-style-4' or
          section.settings.arrow_style == 'arr-circle-style-5'
        %}
            {% render 'smi-icons', icon: 'arrow-left-2' %}
        {% else %}
            {% render 'smi-icons', icon: 'arrow-left' %}
        {% endif %}
      </div>
      <div class="{{ section.settings.arrow_style }} arrow-icon arrow-slider-right smi-cursor-pointer">
        {% if section.settings.arrow_style == 'arr-style-2' or 
          section.settings.arrow_style == 'arr-square-style-4' or
          section.settings.arrow_style == 'arr-square-style-5' or
          section.settings.arrow_style == 'arr-square-style-6' or
          section.settings.arrow_style == 'arr-circle-style-4' or
          section.settings.arrow_style == 'arr-circle-style-5'
        %}
            {% render 'smi-icons', icon: 'arrow-right-2' %}
        {% else %}
            {% render 'smi-icons', icon: 'arrow-right' %}
      {% endif %}
      </div>
    </div>
  </div>
{% endif %}
                    </div>
                </div>
            {% endif %}
        </div>
    </div>
</div>
