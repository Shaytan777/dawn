<style>
    {% assign colorblock = section.blocks | where: 'type', 'color' | first %}
{% if colorblock %}
      #shopify-section-{{ section.id }},
      #shopify-section-{{ section.id }} .smi-color-{{ section.settings.color_scheme }},
      .smi-block-custom-color { 
        --color-background: {{ colorblock.settings.background_color.red }}, {{ colorblock.settings.background_color.green }},{{ colorblock.settings.background_color.blue }};        
        {% if colorblock.settings.background_gradient_color == blank %}
            --gradient-background: {{ colorblock.settings.background_color }};
        {% else %}
            --gradient-background: {{ colorblock.settings.background_gradient_color }};
        {% endif %}
        --color-foreground-hex: {{ colorblock.settings.text_color }};
        --color-foreground: {{ colorblock.settings.text_color.red }}, {{ colorblock.settings.text_color.green }},{{ colorblock.settings.text_color.blue }};
        --color-subtext: {{ colorblock.settings.subtext_color.red }},{{ colorblock.settings.subtext_color.green }},{{ colorblock.settings.subtext_color.blue }};
        --color-button: {{ colorblock.settings.solid_background_button_color.red }}, {{ colorblock.settings.solid_background_button_color.green }},{{ colorblock.settings.solid_background_button_color.blue }};
        --color-button-text: {{ colorblock.settings.solid_button_label_color.red }}, {{ colorblock.settings.solid_button_label_color.green }},{{ colorblock.settings.solid_button_label_color.blue }};
        --color-secondary-button-text: {{ colorblock.settings.outline_button_color.red }}, {{ colorblock.settings.outline_button_color.green }},{{ colorblock.settings.outline_button_color.blue }};
        --color-shadow: {{ colorblock.settings.shadow_color.red }}, {{ colorblock.settings.shadow_color.green }},{{ colorblock.settings.shadow_color.blue }};
    }
{% endif %}
        #shopify-section-{{ section.id }}{
    }
    #smi-{{ section.id }} {
      --section-padding-top: {{ section.settings.padding_top }}px;
--section-padding-bottom: {{ section.settings.padding_bottom }}px;
{% if section.settings.padding_top_section != blank or section.settings.padding_bottom_section != blank %}
    --section-margin-top: {{ section.settings.padding_top_section }}px;
    --section-margin-bottom: {{ section.settings.padding_bottom_section }}px;
{% endif %}
;
      --content-max-width: {{ section.settings.content_max_width }}px;;
      --content-alignment: {{ section.settings.content_alignment }};
      --media-position: {{ section.settings.desktop_media_position }};
  }
</style>
{% liquid
  assign content_alignment = section.settings.content_alignment
  assign color_pos = 0
  assign bkg_pos = 0
  for block in section.blocks
  if block.type == 'color'
  assign color_pos = forloop.index
  endif
  if block.type == 'background'
  assign bkg_pos = forloop.index
  endif
endfor
%}
<div
  id="smi-{{ section.id }}"
  class="smi-{{ section.id }} smi-section__inner smi-gradient smi-section-countdown-1 smi-color-{{ section.settings.color_scheme }} smi-content-settings"
  data-section="countdown-1" data-name="✧Countdown 1">
    
  <input type="hidden" value="" id="interval_countdown_{{ section.id }}">
  {% render 'smi-background' %}
  <div class="smi-container-relative smi-container{%- if section.settings.full_width -%}-fluid{%- endif -%}">
    <div
      class="
      smi-section-block-{{ section.id }}
        smi-container-detail
        {% if section.settings.content_max_width < 576 %}
          {% if section.settings.content_max_width < 400 %}
            min-container
            {% else %}
            pre-min-container
          {% endif %}
        {% endif %}
         smi-content-position-{{ section.settings.content_position }}
      "
      data-block='{
        "color_pos": "{{ color_pos }}", 
        "bkg_pos": "{{ bkg_pos }}",
        "all_block": {{ section.blocks.size }}
      }'
    >
    {%-liquid
    if settings.smi_animations_reveal_on_scroll != nil
        assign animation_scroll = settings.smi_animations_reveal_on_scroll
    else
        assign global_settings = shop.metafields.smind.settings.value
        assign animation_scroll = global_settings.revealOnScroll
    endif
-%}
      <div class="smi-rich-text-detail smi-content-max-width {%- if animation_scroll %} smi-scroll-trigger smi-animate--slide-in{% endif -%}">
        {% for block in section.blocks %}
          <div
            class="
              content-alignment-{{ section.settings.content_alignment }}
              smi-block-postion-{{ forloop.index }}-{{ section.id }}
                smi-rich-content
                {% if block.type == "icon_with_text" %}smi-rich-content-half-{{ block.settings.half_width_on_large_screen }}{% endif %}
                {% if block.type != 'color' and block.type != 'background' %}
                  {% if block.type == 'button' %} smi-mb-md
                  {%else %} 
                  {% comment %} {% if block.type != 'countdown_timer' %} {% endcomment %}
                    smi-mb-lg
                  {% comment %} {% endif %} {% endcomment %}
                {% endif %}
                {% endif %}
                  smi-block-type-{{ block.type }}
              "
              {{ block.shopify_attributes }}
          >
            {% if block.type == 'heading' %}
              <div class="smi-component-blocks-heading content-alignment-{{ content_alignment }}">
  <div class="heading-only heading-only-{{ content_alignment }}">
    <p class="smi-text-sub smi-mb-xxs sub-heading smi-link-wrapper" data-type="subheading" data-key="b.heading.sub_heading">{{ block.settings.sub_heading }}</p>
    <h5 class="{{ block.settings.heading_size }} 
        {% if block.settings.description != blank or block.settings.description != ""%} smi-mb-sm {% endif %} smi-link-wrapper" data-type="heading" data-key="b.heading.heading">{{ block.settings.heading}}</h5>
    <div class="smi-text-md smi-text-75 description smi-link-wrapper" data-field="richtext" data-type="description" data-key="b.heading.description">{{ block.settings.description }}</div>
</div>  
</div>
            {% endif %}
            {% if block.type == 'heading_content' %}
              <div class="smi-component-blocks-heading content-alignment-{{ content_alignment }}">
  <div class="heading-content">
    <h5 class="{{ block.settings.heading_size }} smi-mb-xxs smi-link-wrapper" data-type="heading" data-key="b.heading.heading">{{ block.settings.heading }}</h5>
    <div class="description smi-text-md smi-text-75 smi-link-wrapper" data-field="richtext" data-type="description" data-key="b.heading.content">
    {{ block.settings.content }}
    </div>
</div>
</div>
            {% endif %}
            {% if block.type == 'button' %}
              <div class="smi-component-buttons smi-button-wrapper
    {%- if block.settings.button_full_width == true %} smi-component-buttons-full-width{%- endif -%}
    {%- unless block.settings.first_button_style contains 'text' or block.settings.second_button_style contains 'text' %} smi-component-buttons--same-width{% endunless -%}">
    {% liquid
    assign button_label = block.settings.first_button_label
    assign button_style = block.settings.first_button_style
    assign button_link = block.settings.first_button_link
%}
{% if button_label != blank %}
    <a
            href="{{- button_link -}}"
            class="smi-button smi-button--first smi-text-button smi-button--{{ button_style | split: '_' | first -}}
                {%- if button_style contains 'arrow' %} smi-has-icon{% endif %}
                {{- settings.smi_animations_hover_elements | default: shop.metafields.smind.settings.value.buttonHoverEffect | default: 'none' | prepend: ' smi-hover-' -}}
                {{- ' smi-border smi-shadow' -}}">
            <span>
                {{- button_label | escape -}}
            </span>
        {% if button_style contains 'arrow' %}
            <div class="smi-d-flex smi-align-content-center">
                {% render 'smi-icons', icon: 'arrow-right-2' %}
            </div>
        {% endif %}
    </a>
{% endif %}
    {% liquid
    assign button_label = block.settings.second_button_label
    assign button_style = block.settings.second_button_style
    assign button_link = block.settings.second_button_link
%}
{% if button_label != blank %}
    <a
            href="{{- button_link -}}"
            class="smi-button smi-button--second smi-text-button smi-button--{{ button_style | split: '_' | first -}}
                    {%- if button_style contains 'arrow' %} smi-has-icon{% endif %}
                    {{- settings.smi_animations_hover_elements | default: shop.metafields.smind.settings.value.buttonHoverEffect | default: 'none' | prepend: ' smi-hover-' -}}
                    {{- ' smi-border smi-shadow' -}}">
                    <span>
                        {{- button_label | escape -}}
                    </span>
        {% if button_style contains 'arrow' %}
            <div class="smi-d-flex smi-align-content-center">
                {% render 'smi-icons', icon: 'arrow-right-2' %}
            </div>
        {% endif %}
    </a>
{% endif %}
</div>
            {% endif %}
            {% if block.type == 'countdown_timer' %}
              <div
   {% comment %} style="margin-bottom: calc(var(--spacing-elements) * 24px) !important;" {% endcomment %}
   class="smi-component-settings-countdown-timer 
   {% comment %} smi-mb-lg {% endcomment %}
  {% if content_max_width >= 530 and content_max_width < 610 %}
    small-container-content
    {% elsif content_max_width < 530 and content_max_width >= 400  %}
      extra-small-container-content
      {% elsif content_max_width < 400 %}
        ex-extra-small-container-content
  {% endif %}
    {% if block.settings.countdown_style == "border" %}border-all
    {%elsif block.settings.countdown_style == "border_radius" %}border-all-radius
{% endif %}" id="count_dount_timer_container_{{ section.id }}">
  <input type="hidden" value="{{ block.settings.end_date_time }}" id="count_down_date_{{ section.id }}">
  <input type="hidden" value="{{ block.settings.countdown_style }}" id="style_countdown_{{ section.id }}">
  <input type="hidden" value="{{ block.settings.number_size }}" id="size_countdown_{{ section.id }}">
  <input type="hidden" value="{{ block.settings.hide_expires }}" id="hide_when_expire_{{ section.id }}">
  <div
    class="
    {% if block.settings.countdown_style contains "solid" %}smi-mr-sm{% endif %}
    {% if block.settings.countdown_style == "classic_1" %}
        smi-pr-lg
      {% elsif block.settings.countdown_style == "classic_2" %} 
        smi-pr-lg smi-classic_2
      {% elsif block.settings.countdown_style contains "border" %}
        smi-py-xs smi-pr-lg smi-pl-lg smi-classic_3
      {% elsif block.settings.countdown_style contains "solid_1" %}
    {% endif %}
    "
  >
    <div class="days-countdown countdown
        {% if block.settings.countdown_style contains "solid_1" %}
      smi-px-md smi-py-sm smi-{{ block.settings.countdown_style }} smi-{{ block.settings.countdown_style }}-{{ block.settings.number_size }}
    {% endif %}
        " id="days_count_down">
        <h3 class="{{ block.settings.countdown_style }} {{ block.settings.number_size }}" id="1_day_unit_{{ section.id }}">D</h3>
        <h3 class="{{ block.settings.countdown_style }} {{ block.settings.number_size }}" id="2_day_unit_{{ section.id }}">D</h3>
        <h3 class="{{ block.settings.countdown_style }} {{ block.settings.number_size }} hide-days" id="3_day_unit_{{ section.id }}">D</h3>
        <h3 class="{{ block.settings.countdown_style }} {{ block.settings.number_size }} hide-days" id="4_day_unit_{{ section.id }}">D</h3>
    </div>
    <p class="smi-text-75 unit-countdown smi-text-md">Days</p>
  </div>
  {% unless block.settings.countdown_style contains "classic_2" or block.settings.countdown_style contains "border" %}<span class="smi-text-75">:</span>{% endunless %}
  <div class="
    {% if block.settings.countdown_style contains "solid" %}smi-mr-sm smi-ml-sm{% endif %}
    {% if block.settings.countdown_style == "classic_1" %}
        smi-pr-lg smi-pl-lg
      {% elsif block.settings.countdown_style == "classic_2" %}
        smi-pr-lg smi-pl-lg smi-classic_2
      {% elsif block.settings.countdown_style contains "border" %}
        smi-py-xs smi-pr-lg smi-pl-lg smi-classic_3
