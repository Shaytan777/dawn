                        {% render 'smi-icons', icon: 'plus' %}
                        {% else %}
                    {% render 'smi-icons', icon: 'arrow-down' %}
                    {% endif %}
                </span>
                <span id="collapse_up_arr_{{ forloop.index }}_{{ section.id }}" class="collapse-desc-arr">
                    {% if section.settings.collapsible_type == "2" %}
